{
    "name": "mohammadsalehnia/likeable",
    "description": "Implementing a like system for laravel's Eloquent models.",
    "keywords": [
        "laravel",
        "likeable"
    ],
    "type": "library",
    "autoload": {
        "psr-4": {
            "mohammadsalehnia\\Likeable\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mohammad Salehnia",
            "email": "salehnia.msn@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "mohammadsalehnia\\Likeable\\LikeableServiceProvider"
            ]
        }
    }
}
